                            "id": "36ae2293-5d33-4403-9002-37b3fea2f823",
                            "exec": [
                                "pm.test(\"Create Envelope Happy Path Test\", function () {",
                                "    var jsonData = pm.response.json();",
                                "    pm.expect(jsonData.header.status, \"Test has failed\").to.eql(\"succ\");",
                                "});",
                                "",
                                "pm.test(\"Extract and set ENVELOPE_ID\", function () {",
                                "    var jsonData = pm.response.json();",
                                "    var envelopeId = '\"' + jsonData.message.disbursement_envelope_id + '\"';",
                                "",
                                "    // Set the environment variable",
                                "    pm.environment.set(\"ENVELOPE_ID\", envelopeId);",
                                "",
                                "    // Optional: Log the envelope ID to the console for verification",
                                "    console.log(\"ENVELOPE_ID has been set to: \" + envelopeId);",
                                "});"
                            ],
                            "type": "text/javascript",
                            "packages": {}
                        }
